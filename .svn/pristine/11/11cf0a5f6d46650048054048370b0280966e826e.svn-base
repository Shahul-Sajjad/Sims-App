<div fxLayout="row" fxLayoutAlign="none center">
    <div class="quicklink-nav" *ngFor=" let quickLink of quickLinks; index as i">
        <div style="margin-top: 10px;cursor: pointer;" [ngClass]="{'vertical-line' : i !== 0 }" [routerLink]="['/inbox', quickLink.tag]">
            <p class="quicklink-text "><span>{{quickLink.name}}</span> <br> <span class="quicklink-nav-counts ">{{quickLink.count}}</span> </p>
        </div>
    </div>
</div>

<hr>


