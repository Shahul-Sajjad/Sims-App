.mat-raised-button:not([class*=mat-elevation-z]) {
    margin: 5px;
    font-weight: 300;
    margin-right: 0px;
}
.snackBarColor{
background-color: rgb(29, 102, 29);
  color:lightgreen;
}
.alignBtn{
    position: fixed;
    z-index: 2;
    width: 100%;
    top: 64px;
    min-height: 56px;
    background: white;
}
button{
  border: 2px solid #6f6f6f;
    height: 35px;
    min-width: 95px;
    background: none;
    outline: none;
}
button:hover{
  background:#cecdcd;
}
.iconBtn{
  border: 1px solid;
  background: #607d8b;
  color: white;
  padding-top: 7px;
  border-radius: 0px 6px 6px 0px;
  border-right: 1px solid white;
}